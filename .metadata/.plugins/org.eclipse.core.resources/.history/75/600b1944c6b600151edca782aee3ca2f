package exercises;

public class Exercise11DemonstrateDeadlock {

	public static void main(String[] args) {
		
	}
	
	
	
	private class Target {
		private int size;
		
		public Target() {
			this.size = 5;
		}
	}
	
	private class Bullet {
		private int speed;
		
		public Bullet() {
			this.speed = 100;
		}
	}
}
