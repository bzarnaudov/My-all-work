package all;

public class Problem3ExampleSortingWorkers {

	public static void main(String[] args) {
		Problem1Worker[] workers = new Problem1Worker[10];
		workers[0] = new Problem1Worker("Gosho", "Petkov", 3.16);
		workers[1] = new Problem1Worker("Garo", "Petkov", 5.16);
		workers[2] = new Problem1Worker("Gosho", "Kazanov", 2.16);
		workers[3] = new Problem1Worker("Gergin", "Patkov", 4.16);
		workers[4] = new Problem1Worker("Boyan", "Petkov", 3.56);
		workers[5] = new Problem1Worker("Stavro", "Petkov", 3.66);
		workers[6] = new Problem1Worker("Tigur", "Petkov", 5.46);
		workers[7] = new Problem1Worker("Kzalatik", "Petkov45", 4.66);
		workers[8] = new Problem1Worker("Kako", "Gamen", 3.36);
		workers[9] = new Problem1Worker("Gego", "Gamerov", 5.65);
		
		java.util.Arrays.sort(workers);
		for (Problem1Worker student : workers) {
			System.out.println(student);
		}
	}

}
