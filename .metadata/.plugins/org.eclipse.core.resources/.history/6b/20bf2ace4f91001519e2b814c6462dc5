package problem3FileSystem;

public abstract class File implements Comparable<T> {
	private String name;
	private DateTime dateCreated;
	private DateTime lastChanged;
	private FilesType type;
	private Object content;
	private Directory parrent;

	public File(String name, DateTime dateCreated, DateTime lastChanged,
			Object content, FilesType type) {
		this.name = name;
		this.dateCreated = dateCreated;
		this.lastChanged = lastChanged;
		this.type = type;
		this.content = content;
	}

	public Directory getParrent() {
		return parrent;
	}

	public void setParrent(Directory parrent) {
		this.parrent = parrent;
	}
	
	

}
