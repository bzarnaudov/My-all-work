package all;

public class Problem7AmericanFactory implements Problem7ContinentalFactory {
   
	@Override
	public override HerbivorousAnimal CreateHerbivorousAnimal() {
	        Bison bison = new Bison();
        return bison;
    }

	@Override
    public override CarnivorousAnimal CreateCarnivorousAnimal() {
	        Wolf wolf = new Wolf();
        return wolf;
    } 

}
