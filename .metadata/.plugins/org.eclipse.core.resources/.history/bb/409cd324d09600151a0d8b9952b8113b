package all;

import java.awt.*;
import javax.swing.*;
import java.util.*;

public class StillClock {
	private int hour;
	private int minute;
	private int second;
	
	// Construct a default clock with the current time
	public StillClock() {
		setCurrentTime();
	}
	
	//Construct a clock with specified hour, minute and second
	public StillClock(int hour, int minute, int second) {
		this.hour = hour;
		this.minute = minute;
		this.second = second;
	}
	
	
}
