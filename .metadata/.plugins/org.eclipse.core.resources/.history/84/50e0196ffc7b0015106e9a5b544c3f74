		animals[1] = new Problem5Dog (25, "gogo", "female");
		animals[2] = new Problem5Cat (5, "etko", Problem5Gender.FEMALE);
		animals[3] = new Problem5Cat (53, "gipa", Problem5Gender.MALE);
		animals[4] = new Problem5Dog (12, "skuri", "male");
		animals[5] = new Problem5Frog (68, "Gegov", "female");
		animals[6] = new Problem5Kitten (52, "zlatka", Problem5Gender.MALE);
		animals[7] = new Problem5Tomcat (14, "Galactica", "female");
		animals[8] = new Problem5Frog (10, "Vselena", "male");
		animals[9] = new Problem5Dog (1, "Petkolio", "male");
	}

	
}
