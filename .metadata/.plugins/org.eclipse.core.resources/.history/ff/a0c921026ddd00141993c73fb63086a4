package all;

import chapter11.Cat;
import chapter11.Sequence;

public class CatManipulacia {
	
	private static Cat[] razni1 = new Cat[10];

	public static void main(String[] args) {
		for (int i = 0; i < 10; i++) {
			String p = "Cat" + Sequence.nextValue();
			Cat razni = new Cat(p,p);
			razni.sayMiau();
		}
		

		for (int i = 0; i < 10; i++) {
			razni1[i] = new Cat("Cat" + Sequence.nextValue(), "brown");
		}
		for (int i = 0; i < 10; i++) {
			razni1[i].sayMiau(); 
		}
		
		
	}
}
