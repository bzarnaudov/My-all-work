package all;

public class Problem5Animal {
	private int age;
	private String name;
	private Problem5Gender gender;
	private String sound;
	
	public Problem5Animal()	{
		this(0, "", "");
	}
	
	
	public Problem5Animal(int age, String name, String gender) {
		this.age = age;
		this.name = name;
		this.gender = gender;
	}

	public int getAge() {
		return age;
	}


	public void setAge(int age) {
		this.age = age;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getGender() {
		return gender;
	}


	public void setGender(String gender) {
		this.gender = gender;
	}

	public String getAnimalSound() {
		return sound;
	}


	public void setSound(String sound) {
		this.sound = sound;
	}


	@Override
	public String toString() {
		return "Problem5Animal [age=" + age + ", name=" + name + ", gender="
				+ gender + "] " + this.getAnimalSound();
	}

	
}
